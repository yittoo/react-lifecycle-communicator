{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "sourceMap": true,
    "removeComments": true,
    "declaration": false,
    "noImplicitAny": false,
  },
  "includes": [
    "**/*.ts",
    "**/*.tsx",
  ]
}
